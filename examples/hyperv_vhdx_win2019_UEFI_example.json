{
    "builders": [
        {
            "type": "hyperv-vmcx",
            "clone_from_vm_name": "{{user `vm_name`}}",
            "headless": "true",
            "memory": 2048,
            "cpus" : 1,
            "generation": 2,
            "communicator": "winrm",
            "winrm_username": "Administrator",
            "winrm_password": "SamplePassword",
            "winrm_timeout": "20m",
            "skip_export": "true"
        }
    ],
    "post-processors":[
        {
            "type": "wim",
            "compression": 2
        }
    ]
}